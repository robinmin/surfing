{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "strictNullChecks": true,
    "allowJs": true,
    "baseUrl": ".",
    "paths": {
      "~/*": ["src/*"],
      "@turnstile/client": ["../turnstile/packages/client/src/index.ts"],
      "@turnstile/client/*": ["../turnstile/packages/client/src/*/index.ts", "../turnstile/packages/client/src/*"]
    }
  },
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist/"]
}
