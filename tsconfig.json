{
    "extends": "astro/tsconfigs/base",
    "compilerOptions": {
        "strictNullChecks": true,
        "allowJs": true,
        "baseUrl": ".",
        "paths": {
            "~/*": ["src/*"],
            "@turnstile/client": ["vendor/turnstile/client/index.ts"],
            "@turnstile/client/*": [
                "vendor/turnstile/client/*/index.ts",
                "vendor/turnstile/client/*"
            ]
        }
    },
    "include": [".astro/types.d.ts", "**/*"],
    "exclude": ["dist/"]
}
